  "description": "Symfony bundle to deal with ICS files",
  "type": "symfony-bundle",
  "keywords" : [
    "ical",
    "ics",
    ""
  ],
  "authors": [
    {
      "name": "Georgiana Gligor",
      "email": "gb@tekkie.ro"
    },
    {
      "name"  : "Cloudtroopers PHPers",
      "email" : "php@cloudtroopers.ro"
    }
